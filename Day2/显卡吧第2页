<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:6080719254,&quot;author_name&quot;:&quot;\u5355\u884c\u4e4b\u4eba&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;8c8ee58d95e8a18ce4b98be4baba6c4e&quot;,&quot;first_post_id&quot;:124779608228,&quot;reply_num&quot;:126,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='6080719254' data-thread-type="0" data-floor='7''>
